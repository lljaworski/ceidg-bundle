{
    "name": "lljaworski/ceidg-bundle",
    "description": "Symfony bundle for CEIDG (Centralna Ewidencja i Informacja o Działalności Gospodarczej) integration",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Lukasz Jaworski",
            "email": "kontakt@it42u.pl"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/framework-bundle": "^6.4|^7.0",
        "symfony/http-client": "^6.4|^7.0",
        "symfony/config": "^6.4|^7.0",
        "symfony/dependency-injection": "^6.4|^7.0",
        "api-platform/core": "^3.0|^4.0",
        "symfony/validator": "^6.4|^7.0",
        "symfony/cache": "^6.4|^7.0",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "symfony/phpunit-bridge": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "LukaszJaworski\\CeidgBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LukaszJaworski\\CeidgBundle\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
