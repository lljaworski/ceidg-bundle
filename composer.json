{
    "name": "lukaszjaworski/ceidg-bundle",
    "description": "Symfony bundle for CEIDG (Centralna Ewidencja i Informacja o Działalności Gospodarczej) integration",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Lukasz Jaworski",
            "email": "your-email@example.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "symfony/framework-bundle": "^7.0",
        "symfony/http-client": "^7.0",
        "symfony/config": "^7.0",
        "symfony/dependency-injection": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "symfony/phpunit-bridge": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "LukaszJaworski\\CeidgBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LukaszJaworski\\CeidgBundle\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
